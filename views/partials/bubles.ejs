<div class="chat chat-start" id="{{id}}">
    <div class="chat-header text-primary">{{subject}}</div>
    <div class="chat-bubble min-h-[75px]">
        <h3 class="text-xs text-red-400 opacity-50 mb-2 my-1 me-5">
            <span class="loading loading-ring loading-xs text-current me-1"></span>
            Live Message
        </h3>
        <p class="my-1 break-anywhere"></p>
    </div>
    <div class="chat-footer opacity-50">{{time}}</div>
</div>

<div class="chat chat-end" id="{{id}}">
    <div class="chat-header text-primary">{{subject}}</div>
    <div class="chat-bubble">
        <h3 class="text-xs opacity-50 mb-2 my-1 me-5">
            Live Message Ended
        </h3>
        <p class="my-1 break-anywhere">{{content}}</p>
    </div>
    <div class="chat-footer opacity-50">{{time}}</div>
</div>

