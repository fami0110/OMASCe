<div class="notif w-[max-content] text-center text-xs mx-auto my-3 p-3 bg-base-200 rounded-box">
    <p>{{content}}</p>
</div>

<div class="chat chat-start" id="{{id}}">
    <div class="chat-header text-primary">{{subject}}</div>
    <div class="chat-bubble">{{content}}</div>
    <div class="chat-footer opacity-50">{{time}}</div>
</div>

<div class="chat chat-end" id="{{id}}">
    <div class="chat-header text-primary">{{subject}}</div>
    <div class="chat-bubble">{{content}}</div>
    <div class="chat-footer opacity-50">{{time}}</div>
</div>

